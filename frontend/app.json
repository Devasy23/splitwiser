{
  "expo": {
    "name": "Splitwiser",
    "slug": "splitwiser",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "splitwiser",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2e7d32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2e7d32"
      },
      "package": "splitwiser.app"
    },
    "web": {
      "bundler": "webpack"
    }
  }
}
